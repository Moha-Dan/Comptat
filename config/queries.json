{
	"demande":{
		"yours":{
			"query":"Demands",
			"where":{
				"Demands.author":"@id"
			}
		},
		"yoursCount":{
			"columns":"count(*)",
			"query":"demande.yours"
		},
		"length":{
			"columns":"count(*)",
			"where":"validated = null",
			"query":"Demands"
		},
		"current":{
			"columns":"*",
			"where":"validated = null",
			"query":"Demands"
		},
		"projects":"<option value=\"${Projects.uuid}\" >${Projects.name}</option>"
	},
	"project":{
		"length":{
			"columns":"count(*)",
			"query":"Projects"
		}
	},
	"users":{
		"roles":{
			"columns":"",
			"where":{
				"Users.uuid":"@id"
			},
			"query":"Users"
		},
		"length":{
			"columns":"count(*)",
			"query":"Users"
		}
	},
	"info":{
		"benefit":{
			"columns":"sum(amount) as benefit",
			"query":"Transactions"
		},
		"CA_type":{
			"columns":"TransactionReasons.type as type,sum(amount) as benefit",
			"group":"TransactionReason.type",
			"query":"Transactions",
			"join":["TransactionReasons"],
			"where":"amount>0"
		},
		"regime":{
			"columns":"case when sum(amount)<170000 and type = 0 or type = 2 then 'micro' else 'reel simplfier' end ",
			"query":"info.CA_type"
		},
		"FFOM":{
			"columns":"Projects.name,sum(account),case when sum(account)>1 then 'gain' else 'deficite' end ",
			"query":"Demands",
			"join":["Projects"],
			"group":"Projects.uuid"
		}
	}
}